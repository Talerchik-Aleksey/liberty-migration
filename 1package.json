    "prepare": "husky install",
    "start": "node src/index.js",
    "start-xrp-generator": "node src/xrp-generator.js",
    "dev": "nodemon src/index.js",
    "format": "prettier --write \"src/**/*.js\" \"migrations/**/*.ts\"",
