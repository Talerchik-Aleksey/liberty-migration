{
  "name": "migrations",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "start": "node src/index.js",
    "start-xrp-generator": "node src/xrp-generator.js",
    "dev": "nodemon src/index.js",
    "format": "prettier --write \"src/**/*.js\" \"migrations/**/*.ts\"",
    "migration:create": "npm run typeorm migration:create ./$npm_config_name",
    "migration:run": "npm run typeorm migration:run -- -d ./data-source.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d ./data-source.ts",
    "migration:generate": "npm run typeorm migration:generate -- -d ./data-source.ts ./migrations/post-refactoring/latest-changes-migration",
    "test": "jest --passWithNoTests",
    "typeorm": "typeorm-ts-node-esm"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.12devs.com:liberty-meets/migrations.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "config": "^3.3.8",
    "pg": "^8.8.0",
    "typeorm": "^0.3.11",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/config": "3.3.0",
    "@types/jest": "28.1.4",
    "@types/node": "^16.11.10",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.20",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  }
}
